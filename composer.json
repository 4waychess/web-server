{
    "name": "fourwaychess/web-server",
    "description": "The adless and forever free 4 player chess server.",
    "license": "GPL-3.0-or-later",
    "keywords": [],
    "require": {
        "hybridauth/hybridauth": "^3.7",
        "paragonie/halite": "^4.8",
        "paragonie/sapient": "^1.1",
        "paragonie/stern": "^1.0",
        "symfony/cache": "^5.3",
        "symfony/config": "^5.3",
        "symfony/dependency-injection": "^5.3",
        "symfony/dotenv": "^5.3",
        "symfony/error-handler": "^5.3",
        "symfony/form": "^5.3",
        "symfony/http-client": "^5.3",
        "symfony/http-foundation": "^5.3",
        "symfony/mailer": "^5.3",
        "symfony/messenger": "^5.3",
        "symfony/mime": "^5.3",
        "symfony/password-hasher": "^5.3",
        "symfony/polyfill-apcu": "^1.23",
        "symfony/polyfill-ctype": "^1.23",
        "symfony/polyfill-mbstring": "^1.23",
        "symfony/rate-limiter": "^5.3",
        "symfony/security-csrf": "^5.3",
        "symfony/serializer": "^5.3",
        "symfony/templating": "^5.3",
        "symfony/translation": "^5.3",
        "symfony/validator": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": { "FourWayChess\\": "./src" }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "prefer-stable": true
